{
    "name": "<%= slugname %>",
    "version": "0.0.0",
    "description": "<%= props.description %>",<% if (props.homepage) { %>
    "homepage": "<%= props.homepage %>",<% } %>
    "author": {
        "name": "<%= props.authorName %>",
        "email": "<%= props.authorEmail %>"<% if (props.authorUrl) { %>,
        "url": "<%= props.authorUrl %>"<% } %>
    },
    "maintainers": [<% for (var i = 0; i < maintainers.length; i++) { %>
        "<%= maintainers[i] %>"<% } %>
    ],
    "repository": {
        "type": "git",
        "url": "<%= repoUrl %>"
    },
    "bugs": {
        "url": "<%= repoUrl %>/issues"
    },
    "license": "<%= props.license %>",<% if (props.license === "MIT") { %>
    "licenses": [
        {
           "type": "MIT",
           "url": "<%= repoUrl %>/blob/master/LICENSE"
        }
    ],<% } %>
    "keywords": [
        "<%= slugname %>"<% for (var i = 0; i < keywords.length; i++) { %>,
        "<%= keywords[i] %>"<% } %>
    ], <% if (isNpmPackage) { %>
    "main": "lib/<%= slugname %>.js",
    "directories": {
       "lib": "./lib"
    },<% } %>
    "engines": {
        "node": ">=0.10.0"
    },
    "scripts": {
        "test": "grunt test"
    },
    "dependencies": {

    },
    "devDependencies": {
        "grunt": "^0.4.5",
        "grunt-bg-shell": "^2.3.1",
        "grunt-bump": "^0.0.14",
        "grunt-contrib-clean": "^0.5.0",<% if (isBowerPackage) { %>
        "grunt-contrib-concat": "^0.4.0",<% } %>
        "grunt-contrib-jshint": "^0.10.0",<% if (isBowerPackage) { %>
        "grunt-contrib-uglify": "^0.5.0",<% } %>
        "grunt-conventional-changelog": "^1.1.0",<% if (isNpmPackage) { %>
        "grunt-jasmine-node": "^0.2.1",<% } %><% if (props.useKarma) { %>
        "grunt-karma": "^0.8.3",<% } %><% if (props.useAngular) { %>
        "grunt-ngmin": "^0.0.3",<% } %>
        "grunt-open": "^0.2.3",
        "grunt-release": "^0.7.0",<% if (isNpmPackage) { %>
        "istanbul": "^0.2.13",<% } %><% if (props.useKarma) { %>
        "karma-detect-browsers": "*",
        "karma-coverage": "*",
        "karma-jasmine": "^0.2.2",
        "karma-junit-reporter": "*",
        "karma-mocha-reporter": "*",<% } %>
        "load-grunt-tasks": "^0.6.0",
        "time-grunt": "^0.3.2"
    }
}